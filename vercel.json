{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node@latest"
    }
  ],
  "routes": [
    {
      "src": "/proxy/:path*",
      "dest": "/proxy/:path*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "https://howtoai.tech/"
      }
    }
  ]
}
